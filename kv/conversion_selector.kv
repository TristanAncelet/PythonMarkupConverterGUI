#: set items ["A","B", "C"]
<ResizingBoxlayout@BoxLayout>:
  pos_hint: { "top":1 }
  padding: 4
  size_hint: 1,None
  height: self.minimum_height
  orientation: "vertical"

  
<Selector@BoxLayout>:
  padding: 2
  size_hint_y: None
  orientation: "horizontal"
  height: 50
  
<ConversionSelector@MDList>:
  pos_hint: {"top":1}

  canvas.before:
    Rectangle:
      size: self.size
      pos: self.pos
    
    Color: 
      rgba: 0.5,0.7,0.7,1
xz
  OneLineListItem:
    text: "To: N/A"
    on_press:
      print(self.text + " was pressed")

  OneLineListItem:
    text: "From: N/A"
    on_press:
      print(self.text + " was pressed")





  